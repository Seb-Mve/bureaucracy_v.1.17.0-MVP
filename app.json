{
  "expo": {
    "name": "BUREAUCRACY++",
    "slug": "bureaucracy-mvp",
    "version": "1.17.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "bureaucracy",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": ["expo-router", "expo-font", "expo-web-browser"],
    "experiments": {
      "typedRoutes": true
    }
  }
}
